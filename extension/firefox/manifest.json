{
  "name": "NNIN Generator",
  "version": "1.3",
  "description": "Generate norwegian national identity numbers",
  "permissions": ["storage", "activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "nnin32.png"
